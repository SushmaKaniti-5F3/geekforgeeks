
# User function Template for python3

class Solution:
    
    #Function to reverse words in a given string.
    def reverseWords(self,S):
        # code here
        s=S.split(".")
        j=len(s)-1;
        a=[]
        while(j>=0):
            a.append(s[j])
            j-=1
        l=".".join(a)
        return l;
            
        

#{ 
#  Driver Code Starts
# Initial Template for Python 3

if __name__ == '__main__':
    t = int(input())
    for i in range(t):
        s = str(input())
        obj = Solution()
        print(obj.reverseWords(s))

# } Driver Code Ends
