class Solution{
    public:
    void evenOdd(int a, int b){
        vector<int>ans;
        if(a%2==0)
        {
            ans.push_back(a);
            ans.push_back(b);
        }
        else
        {
            ans.push_back(b);
            ans.push_back(a);
        }
        for(auto i :ans)
        cout<<i<<"\n";
    }
};
