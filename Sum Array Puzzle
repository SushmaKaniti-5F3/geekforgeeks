void SumArray(int arr[], int n)
{
    // you code here
    int sum;
    int a[n];
    for(int i=0;i<n;i++)
    {
        sum=0;
        for(int j=0;j<n;j++)
        {
            if(i==j)continue;
            sum+=arr[j];
        }
        a[i]=sum;
    }
    for(int i=0;i<n;i++)
    {
        arr[i]=a[i];
    }
}
