#include<bits/stdc++.h>
using namespace std;
class Solution
{
    int checkPerfectSquare(int N){
        float temp=pow(N,0.5);
        if(temp==int(temp))
        return 1;
        else
        return 0;
    }
};
int main()
{
  int t;
  cin>>t;
  while(t--)
  {
    long long n;
    cin>>n;
    Solution ob;
    cout<<ob.checkPerfectSquare(N)<<endl;
  }
  return 0;
}

